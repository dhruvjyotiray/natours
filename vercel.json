{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs14.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1",
      "continue": true
    }
  ]
}
